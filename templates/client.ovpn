client

proto tcp
remote {{address}} {{port}}
comp-lzo
dev tun

resolv-retry infinite
auth-retry none
nobind
persist-key
persist-tun
# ns-cert-type server
verb 3
mute 20
mute-replay-warnings

# Certificate Authority cert
<ca>
{{ca -}}
</ca>
# Client cert
<cert>
{{cert -}}
</cert>
# Client private key 
<key>
{{key -}}
</key>

# PSK and direction of TLS channel authentication
<tls-auth>
{{tls_auth -}}
</tls-auth>
key-direction 1
